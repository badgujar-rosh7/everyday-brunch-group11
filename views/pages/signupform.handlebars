{{!signupform}}
<h1>test</h1>
<section class="section loginform-section">
  <div class="container">
    <div class="main-text">
      <h2 class="mt-title text-center mt-5 pt-5 pb-10">Sign Up for an Account<h2>
      <h5>
      <div class="mt-3 f5 ph20 pb-5 mt-body">Start your Everyday Brunch today</div>
      <div class="mt-3 signup-form">
          <form class="row g-3">
            <div class="col-md-6">
              <label for="inputEmail4" class="form-label">Email</label>
              <input type="email" class="form-control" id="inputEmail4">
            </div>
            <div class="col-md-6">
              <label for="inputPassword4" class="form-label">Password</label>
              <input type="password" class="form-control" id="inputPassword4">
            </div>
            <div class="mt-5 col-md-4">
              <label for="validationDefault01" class="form-label">First name</label>
              <input type="text" class="form-control" id="validationDefault01" required>
            </div>
            <div class="mt-5 col-md-4">
              <label for="validationDefault02" class="form-label">Last name</label>
              <input type="text" class="form-control" id="validationDefault02" required>
            </div>
            <div class="mt-5 col-md-4">
              <label for="validationDefaultUsername" class="form-label">Username</label>
              <div class="input-group">
                <span class="input-group-text" id="inputGroupPrepend2">@</span>
                <input type="text" class="form-control" id="validationDefaultUsername"  aria-describedby="inputGroupPrepend2" required>
              </div>
            </div>
            <div class="col-md-6">
              <label for="validationDefault03" class="form-label">City</label>
              <input type="text" class="form-control" id="validationDefault03" required>
            </div>
            <div class="col-md-3">
              <label for="validationDefault04" class="form-label">State</label>
              <select class="form-select" id="validationDefault04" required>
                <option selected disabled value="">Choose...</option>
                <option>...</option>
              </select>
            </div>
            <div class="col-md-3">
              <label for="validationDefault05" class="form-label">Date of Birth</label>
              <input type="text" class="form-control" id="validationDefault05" value="MM/DD/YYYY" style="color: #777;" required>
            </div>
            <div class="col-12">
              <div class="form-check">
                <input class="mt-3 form-check-input" type="checkbox" value="" id="invalidCheck2" required>
                <h6>
                  <label class="mt-3 form-check-label" for="invalidCheck2" >
                    I agree to all terms and conditions.
                  </label>
                </h6>
              </div>
            </div>
            <div class="mt-5 d-grid gap-2 col-7 mx-auto">
              <button class="btn btn-primary" type="button">Sign Up</button>
          </div>
          </form>
        </div>
        <div class="mt-5 mb-3 pb-5">
          <span class="mr5">Already have an account?</span>
          <span>
            <a class="js-toggle-to-log-in blue-text-link" href="#">Log In</a>
          </span>
        </div>
      </h5>
    </div>
  </div>
  {{!signupform}}